<%- if controller_name != 'sessions' %>
<div class="userspace">
  <%= link_to "多Ya cuentas con una cuenta? Ingresa", new_session_path(resource_name), class:"linkDevise" %><br />	
</div>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
<div class="userspace">
  <%= link_to "多Eres nuevo en WinFitt? Registrate", new_registration_path(resource_name), class:"linkDevise" %><br />	
</div>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
<div class="userspace">
  <%= link_to "多Olvidaste tu contrase単a?", new_password_path(resource_name), class:"linkDevise" %><br />	
</div>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
<div class="userspace">
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class:"linkDevise" %><br />	
</div>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
<div class="userspace">
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class:"linkDevise" %><br />	
</div>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
  <div class="userspace">
     <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class:"linkDevise" %><br /> 	
  </div>
  <% end -%>

<% end -%>
